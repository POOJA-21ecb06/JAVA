import java.util.*;
public class armstrong
{
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
double sum=0;
int count=0;
int d=0;
int num=n;
int num1=num;
while(n!=0){
    count++;
    n=n/10;
    
}
for(int i=0;i<count;i++){
    d=num%10;
    sum=sum+(Math.pow(d,count));
    num=num/10;
}
if(sum==num1){
System.out.println("ARMSTRONG");
}else
{
System.out.println("NOT_ARMSTRONG");
}
}
}
