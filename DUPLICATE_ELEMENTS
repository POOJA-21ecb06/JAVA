import java.util.*;
class duplicate {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=10;
int arr[]={0,1,2,2,3,4,5,3,2,1,2,3,6};
int count=0;
for(int i=0;i<arr.length;i++){
   for(int j=i+1;j<arr.length;j++){
       if(arr[i]==arr[j]){
           count++;
           break;
        }
    }
   }
System.out.print(count);
            
}
}
