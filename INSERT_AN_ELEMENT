import java.util.*;
public class insert {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
int k=sc.nextInt();
//int num=sc.nextInt();
int num=0;
int temp=0;
int temp2=0;
int arr[]=new int[n];
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}
for(int i=0;i<n;i++){
    if(k==(i+1)){
        num=arr[i]; 
        arr[i]=arr[i]+1;
        i++;
    }
    System.out.print(arr[i]);
    }
}


}
