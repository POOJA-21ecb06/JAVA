import java.util.*;
public class insertt {
 public static void main(String[] args) {
Scanner s = new Scanner(System.in);
int n = s.nextInt();
int pos = s.nextInt();
int num = s.nextInt();
int a[] = new int[n+1];
for(int i = 0; i < n; i++)
{
a[i] = s.nextInt();
}

for(int i = (n-1); i >= (pos-1); i--)
{
a[i+1] = a[i];
}
a[pos-1] = num;
for(int i = 0; i <=n; i++)
{
System.out.print(a[i]);
}
}
 
}
