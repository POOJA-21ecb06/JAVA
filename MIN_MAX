import java.util.*;
class minmax {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
int arr[]=new int[n];
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}
for(int i=0;i<n;i++){
    Arrays.sort(arr);
}
System.out.print("Maximum"+arr[n-1]+"minimum"+arr[0]);
}
}
