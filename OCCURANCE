import java.util.*;
class occurance {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
int x=sc.nextInt();
int arr[]=new int[n];
int count=0;
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}

for(int i=0;i<n;i++){
   if(x==arr[i]){
       count++;
    }
}
{
    if(count==0){
            System.out.print("0");
        }
        else{
    System.out.print(count);
    }
    }
}
}
