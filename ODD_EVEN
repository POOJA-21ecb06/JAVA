import java.util.*;
class oddven {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
int arr[]=new int[n];
int eve=0;
int odd=0;
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}
for(int i=0;i<n;i++){
    if(i%2==0){
    eve=eve+arr[i];
}else{
    odd=odd+arr[i];
}
}
System.out.print(eve);
System.out.print(odd);
}
}
