import java.util.*;
class odeve {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
int arr[]=new int[n];
int eve=0;
int odd=0;
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}
for(int i=0;i<n;i++){
    if(arr[i]%2!=0){
        System.out.print(arr[i]);
    }
}
    for(int j=0;j<n;j++){
      if(arr[j]%2==0){
        System.out.print(arr[j]);
    }  
    }
}


}
