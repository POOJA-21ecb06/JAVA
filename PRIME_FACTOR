import java.util.*;
public class primefactor
{
public static void main(String[] args) 
{
Scanner sc = new Scanner(System.in);
System.out.print("enter input");
int n=sc.nextInt();
int sum=0;
int d=0;
int s=0;
int k=0;
for(int i=1;i<n;i++)
{
    if(n%i==0){
        sum=sum+i;
        }
}
while(sum!=0){
    d=sum%10;
    s=s+d;
    sum=sum/10;
}
if(s>=10){
    d=s%10;
    k=k+d;
    s=s/10;
}
System.out.print("the output");
System.out.println(s);
}
}

 
