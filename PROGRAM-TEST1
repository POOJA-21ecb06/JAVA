//KILLING QUESITION IN HACKEREARTH
import java.util.*;
class killing {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n = 10;
int j=0;
int sum=0;
int count=0;
int k=0;
for(int i=1;i<=n;i++){
        j=j+i*i;
        if (j<=n){
            count++;
        }

}
System.out.print(count);

}
}
