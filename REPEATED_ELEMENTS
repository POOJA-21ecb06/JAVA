import java.util.*;
class repeat {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
int n=sc.nextInt();
int k=sc.nextInt();
int arr[]=new int[n];
int count=0;
int a=0;
int start=0,end=0;
for(int i=0;i<n;i++){
    arr[i]=sc.nextInt();
}
for(int i=0;i<n;i++){
if(k==arr[i]){
    if (a==0)
    start=i;
    a++;
}
end=i;
count++;
}
System.out.print(start);
System.out.print(end);   
System.out.print(count);
}
}
