import java.util.*;
class STRING_PATTERN {
 public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
String str=sc.nextLine();
int l=str.length();
int m=(l/2);
for(int i=0;i<l;i++){
    for(int j=0;j<l;j++){
        if(i==m){
            System.out.print(str.charAt(j));
        }else if(j==m){
            System.out.print(str.charAt(i));
        }else{
            System.out.print(" ");
        }
    }
    System.out.println(" ");
}
}
}
